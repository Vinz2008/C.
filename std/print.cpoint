include libc.hpoint
include capi.hpoint

func printd(d : double) void{
	printd_internal(d)
}

func printi(i : int) void {
	printi_internal(i)
}

func putchard(d : double) void {
	putchard_internal(d)
}

func printstr(x : i8 ptr) void{
	puts(x)
}

func printc(c : i8) void {
	printf("%c", c)
}

test "testing printd" {
	var a = 2
	printd(a)
	printd(a)
}

test "testing printi" {
	printi(3)
	printf("hello world\n")
}