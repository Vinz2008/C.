import @std/print.cpoint
extern open(pathname : i8 ptr flags : int) int;
extern write(fd : int buf : i8 ptr count : int) int;
extern close(fd : int) int;
func main(){
    var flags : int = 01
    var fd : int = open("test_io.txt", flags)
    printstr("fd : ")
    printi(fd)
    var nb_char : int = 17
    var out : int = write(fd, "test hello world", nb_char)
    printstr("write output")
    printi(out)
    close(fd)
}