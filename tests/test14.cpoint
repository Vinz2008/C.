import @std/print.cpoint
import @std/file.cpoint

func main(){
    var fd : int = file::open("test_io.txt")
    printstr("fd : ")
    printi(fd)
    var out : int = file::write(fd, "test hello world\n")
    printstr("write output")
    printi(out)
    file::close(fd)
    var path = file::canonicalize("../tests")
    printstr(path)
    file::symlink("test14.cpoint", "symlinked_test14.cpoint.temp")
}